{
  "expo": {
    "owner": "ry187-mobile",
    "name": "HOAApp",
    "slug": "HOAApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-facebook"
    ],
    "facebookAppId": "REPLACE_WITH_YOUR_REAL_FACEBOOK_APP_ID",
    "facebookDisplayName": "HOAApp"
  }
}
